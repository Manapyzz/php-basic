{% extends 'base.html.twig' %}

{% block content %}
    <form action="" method="POST">
        <label for="lastname">Lastname</label>
        <input type="text" id="lastname" name="lastname" placeholder="Sanchez" required>
        <label for="firstname">Firstname</label>
        <input type="text" id="firstname" name="firstname" placeholder="Rick" required>
        <label for="email">Email</label>
        <input type="text" id="email" name="email" placeholder="ricksanchez@gmail.com" required>
        <label for="password">Password</label>
        <input type="text" id="password" name="password" placeholder="123456789" required>
        <input type="submit" value="register">
    </form>
{% endblock %}